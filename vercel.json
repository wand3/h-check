{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
 "installCommand": "CFLAGS='-g0 -Wl' pip install -r requirements.txt --no-cache-dir --no-binary numpy"
}

